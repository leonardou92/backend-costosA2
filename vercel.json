{
  "version": 2,
  "builds": [
    { "src": "api/**/*.cjs", "use": "@vercel/node" },
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.cjs" },
    { "src": "/api", "dest": "/api/index.cjs" }
  ]
  ,
  "functions": {
    "api/**/*.cjs": { "memory": 1024, "maxDuration": 60 },
    "api/**/*.js": { "memory": 1024, "maxDuration": 60 }
  }
}
